        <Button x:Name="button_zaloguj" Content="Zaloguj" HorizontalAlignment="Left" Margin="288,180,0,0" VerticalAlignment="Top" Width="75" Click="button_zaloguj_Click"/>
        <Button x:Name="button_zarejestruj" Content="Zarejestruj się" HorizontalAlignment="Left" Margin="117,180,0,0" VerticalAlignment="Top" Width="87" Click="button_zarejestruj_Click"/>

    </Grid>
