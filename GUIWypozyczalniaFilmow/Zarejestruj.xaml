<Window x:Class="GUIWypozyczalniaFilmow.Zarejestruj"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUIWypozyczalniaFilmow"
        mc:Ignorable="d"
        Title="Zarejestruj" Height="450" Width="800">
    <Grid>
        <Label x:Name="label_imie" Content="Imię" HorizontalAlignment="Left" Margin="110,38,0,0" VerticalAlignment="Top"/>
