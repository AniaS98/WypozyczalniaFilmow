        <Label x:Name="label3_login" Content="Login" HorizontalAlignment="Left" Margin="110,224,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4_haslo" Content="Hasło" HorizontalAlignment="Left" Margin="110,275,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label5_powtorzHaslo" Content="Powtórz Hasło" HorizontalAlignment="Left" Margin="110,327,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_imie" HorizontalAlignment="Left" Height="23" Margin="278,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1_nazwisko" HorizontalAlignment="Left" Height="23" Margin="278,83,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
