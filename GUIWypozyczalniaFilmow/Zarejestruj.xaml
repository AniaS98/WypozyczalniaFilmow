        <TextBox x:Name="textBox5_poetorzHaslo" HorizontalAlignment="Left" Height="23" Margin="278,330,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button_zaluzKonto" Content="Załóż konto" HorizontalAlignment="Left" Margin="323,374,0,0" VerticalAlignment="Top" Width="75" Click="button_zaluzKonto_Click"/>
        <Label x:Name="label6_mail" Content="Mail" HorizontalAlignment="Left" Margin="110,174,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox6_mail" HorizontalAlignment="Left" Height="23" Margin="278,177,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
